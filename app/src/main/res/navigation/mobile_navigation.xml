        <argument
            android:name="tipo_menu"
            app:argType="integer" />
        <action
            android:id="@+id/action_menuOpcionesCategoriaFragment_to_menuPrincipalFragment"
            app:destination="@id/menuPrincipalFragment" />
    </fragment>
</navigation>