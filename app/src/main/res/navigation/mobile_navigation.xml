        <argument
            android:name="tipo_formulario"
            app:argType="integer" />
        <action
            android:id="@+id/action_bajasFragment_to_menuPrincipalFragment"
            app:destination="@id/menuPrincipalFragment" />
    </fragment>
    <fragment
        android:id="@+id/listarFragment"
        android:name="com.example.proyectoampliacion.Fragmentos.Mantenimiento.ListarFragment"
