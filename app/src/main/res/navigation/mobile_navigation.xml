        android:label="fragment_modificar"
        tools:layout="@layout/fragment_modificar" >
        <argument android:name="tipo_formulario" />
        <action
            android:id="@+id/action_modificarFragment_to_menuPrincipalFragment"
            app:destination="@id/menuPrincipalFragment" />
    </fragment>
